{
  "semi": true,
  "trailingComma": "es5",
  "singleQuote": true,
  "tabWidth": 2,
  "printWidth": 90,
  "useTabs": false,
  "bracketSpacing": true,
  "bracketSameLine": false,
  "importOrder": [
    "^@app/(.*)$",
    "^@libs/(.*)$",
    "^@shared/(.*)$",
    "^@utils/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "plugins": ["@trivago/prettier-plugin-sort-imports", "prettier-plugin-tailwindcss"],
  "pluginSearchDirs": false
}
